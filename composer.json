{
  "name": "zeroseven/z7-blog-rss",
  "license": "GPL-2.0-only",
  "type": "typo3-cms-extension",
  "description": "RSS-feed for extension z7_blog",
  "authors": [
    {
      "name": "zeroseven design studios GmbH",
      "email": "typo3@zeroseven.de",
      "homepage": "https://www.zeroseven.de/",
      "role": "Company"
    },
    {
      "name": "Raphael Thanner",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ^11.0",
    "zeroseven/z7-blog": "~1.0.0 || ~1.1.0 || ~1.2.0 || ~1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Zeroseven\\Z7BlogRss\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "z7_blog_rss"
    },
    "scripts": {
      "prepare-release": [
        "rm .editorconfig",
        "rm .gitattributes",
        "rm .gitignore",
        "rm -rf .github",
        "rm .gitlab-ci.yml",
      ]
    }
  }
}
